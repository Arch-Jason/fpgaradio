--synthesis_resources = lut 1 
SUBDESIGN mux_smb
( 
	data[1..0]	:	input;
	result[0..0]	:	output;
	sel[0..0]	:	input;
) 
VARIABLE 
	result_node[0..0]	: WIRE;
	sel_node[0..0]	: WIRE;
	w_data42w[1..0]	: WIRE;

BEGIN 
	result[] = result_node[];
	result_node[] = ( ((sel_node[] & w_data42w[1..1]) # ((! sel_node[]) & w_data42w[0..0])));
	sel_node[] = ( sel[0..0]);
	w_data42w[] = ( data[1..0]);
END;
--VALID FILE
